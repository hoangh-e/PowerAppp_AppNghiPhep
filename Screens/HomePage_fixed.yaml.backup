Screens:
  HomePage_1:
    Properties:
      Fill: =RGBA(243, 243, 243, 1)
      LoadingSpinnerColor: =RGBA(0, 120, 212, 1)
      OnVisible: |-
        =Set(varInitialized, true);
        ClearCollect(
          colLeaveRequests,
          Filter(
            LeaveRequests,
            Id = LeaveRequestID && 
            Title = Title && 
            Status = Status && 
            StartDate = StartDate && 
            EndDate = EndDate
          )
        );
    Children:
      - HeaderComponent_1:
          Control: CanvasComponent
          ComponentName: HeaderComponent
          Properties:
            X: =0
            Y: =0
            Width: =Parent.Width
            Height: =60
            ZIndex: =1
            Title: ="Thực hiện lập kế hoạch lựa chọn nhà thầu"
      
      - MainContent:
          Control: GroupContainer
          Variant: ManualLayout
          Properties:
            X: =0
            Y: =61
            Width: =Parent.Width
            Height: =Parent.Height - 61
            ZIndex: =2
            Fill: =RGBA(255, 255, 255, 1)
          Children:
            - BreadcrumbContainer:
                Control: GroupContainer
                Variant: ManualLayout
                Properties:
                  X: =0
                  Y: =0
                  Width: =Parent.Width
                  Height: =40
                  ZIndex: =1
                  Fill: =RGBA(255, 255, 255, 1)
                  BorderColor: =RGBA(230, 230, 230, 1)
                  BorderThickness: =1
                Children:
                  - lblBreadcrumb:
                      Control: Label
                      Properties:
                        X: =50
                        Y: =10
                        Width: =Parent.Width - 100
                        Height: =20
                        ZIndex: =1
                        Text: ="Quy trình Thực hiện lập kế hoạch lựa chọn nhà thầu / Tạo dự thảo"
                        Size: =12
                        Color: =RGBA(80, 80, 80, 1)
            
            - ProcessStatusSection:
                Control: GroupContainer
                Variant: ManualLayout
                Properties:
                  X: =50
                  Y: =60
                  Width: =250
                  Height: =Parent.Height - 80
                  ZIndex: =2
                  Fill: =RGBA(255, 255, 255, 1)
                Children:
                  - lblProcessStatus:
                      Control: Label
                      Properties:
                        X: =0
                        Y: =0
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =1
                        Text: ="Trạng thái quy trình"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - ProcessSteps:
                      Control: GroupContainer
                      Variant: ManualLayout
                      Properties:
                        X: =0
                        Y: =40
                        Width: =Parent.Width
                        Height: =Parent.Height - 50
                        ZIndex: =2
                        Fill: =RGBA(255, 255, 255, 1)
                      Children:
                        - Step1:
                            Control: CanvasComponent
                            ComponentName: ProcessStepComponent
                            Properties:
                              X: =0
                              Y: =0
                              Width: =Parent.Width
                              Height: =100
                              ZIndex: =1
                              StepNumber: =1
                              StepTitle: ="Dự thảo Tờ trình, Gửi thẩm ý kiến"
                              StepStatus: ="Completed"
                              IsActive: =true
                        
                        - Step2:
                            Control: CanvasComponent
                            ComponentName: ProcessStepComponent
                            Properties:
                              X: =0
                              Y: =110
                              Width: =Parent.Width
                              Height: =50
                              ZIndex: =2
                              StepNumber: =2
                              StepTitle: ="Dự thảo BCTR"
                              StepStatus: ="Pending"
                              IsActive: =false
                        
                        - Step3:
                            Control: CanvasComponent
                            ComponentName: ProcessStepComponent
                            Properties:
                              X: =0
                              Y: =170
                              Width: =Parent.Width
                              Height: =50
                              ZIndex: =3
                              StepNumber: =3
                              StepTitle: ="Trình Lãnh đạo phê duyệt"
                              StepStatus: ="Pending"
                              IsActive: =false
                        
                        - Step4:
                            Control: CanvasComponent
                            ComponentName: ProcessStepComponent
                            Properties:
                              X: =0
                              Y: =230
                              Width: =Parent.Width
                              Height: =50
                              ZIndex: =4
                              StepNumber: =4
                              StepTitle: ="Hoàn thành"
                              StepStatus: ="Pending"
                              IsActive: =false
            
            - FormSection:
                Control: GroupContainer
                Variant: ManualLayout
                Properties:
                  Name: ="FormSection"
                  X: =320
                  Y: =60
                  Width: =Parent.Width - 350
                  Height: =Parent.Height - 80
                  ZIndex: =3
                  Fill: =RGBA(255, 255, 255, 1)
                Children:
                  - lblFormTitle:
                      Control: Label
                      Properties:
                        Name: ="lblFormTitle"
                        X: =0
                        Y: =0
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =1
                        Text: ="Tiêu đề"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - txtTitle:
                      Control: TextInput
                      Properties:
                        Name: ="txtTitle"
                        X: =0
                        Y: =40
                        Width: =Parent.Width
                        Height: =40
                        ZIndex: =2
                        Default: =""
                        Tooltip: ="Dự thảo văn bản Hồ sơ mời thầu"
                        BorderColor: =RGBA(166, 166, 166, 1)
                        PlaceholderText: ="Dự thảo văn bản Hồ sơ mời thầu"
                  
                  - lblNotes:
                      Control: Label
                      Properties:
                        Name: ="lblNotes"
                        X: =0
                        Y: =90
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =3
                        Text: ="Ghi chú"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - txtNotes:
                      Control: TextInput
                      Properties:
                        Name: ="txtNotes"
                        X: =0
                        Y: =130
                        Width: =Parent.Width
                        Height: =100
                        ZIndex: =4
                        Mode: =TextMode.MultiLine
                        Default: =""
                        BorderColor: =RGBA(166, 166, 166, 1)
                        PlaceholderText: ="Nhập ghi chú..."
                  
                  - lblUploadDocuments:
                      Control: Label
                      Properties:
                        Name: ="lblUploadDocuments"
                        X: =0
                        Y: =250
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =5
                        Text: ="Tài liệu đính kèm"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - DocumentUploadComponent_1:
                      Control: CanvasComponent
                      ComponentName: DocumentUploadComponent
                      Properties:
                        X: =0
                        Y: =290
                        Width: =Parent.Width
                        Height: =150
                        ZIndex: =6
                  
                  - DocumentTableComponent_1:
                      Control: CanvasComponent
                      ComponentName: DocumentTableComponent
                      Properties:
                        X: =0
                        Y: =450
                        Width: =Parent.Width
                        Height: =200
                        ZIndex: =7
                  
                  - lblReviewers:
                      Control: Label
                      Properties:
                        Name: ="lblReviewers"
                        X: =0
                        Y: =670
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =8
                        Text: ="Người thẩm định"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - ReviewerTableComponent_1:
                      Control: CanvasComponent
                      ComponentName: ReviewerTableComponent
                      Properties:
                        X: =0
                        Y: =710
                        Width: =Parent.Width
                        Height: =150
                        ZIndex: =9
                  
                  - lblCC:
                      Control: Label
                      Properties:
                        Name: ="lblCC"
                        X: =0
                        Y: =880
                        Width: =Parent.Width
                        Height: =30
                        ZIndex: =10
                        Text: ="Người được CC"
                        FontWeight: =FontWeight.Bold
                        Size: =14
                  
                  - CCTableComponent_1:
                      Control: CanvasComponent
                      ComponentName: CCTableComponent
                      Properties:
                        X: =0
                        Y: =920
                        Width: =Parent.Width
                        Height: =150
                        ZIndex: =11
                  
                  - btnSubmitContainer:
                      Control: GroupContainer
                      Variant: ManualLayout
                      Properties:
                        Name: ="btnSubmitContainer"
                        X: =0
                        Y: =1090
                        Width: =Parent.Width
                        Height: =50
                        ZIndex: =12
                        Fill: =RGBA(255, 255, 255, 1)
                      Children:
                        - btnSubmit:
                            Control: Button
                            Properties:
                              Name: ="btnSubmit"
                              X: =Parent.Width - 150
                              Y: =0
                              Width: =150
                              Height: =40
                              ZIndex: =1
                              Text: ="Gửi"
                              Fill: =RGBA(0, 120, 212, 1)
                              Color: =RGBA(255, 255, 255, 1)
                              OnSelect: =Navigate(Screen2) 