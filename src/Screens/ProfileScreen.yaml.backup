Screens:
  ProfileScreen:
    Properties:
      Fill: =RGBA(248, 250, 252, 1)
      OnVisible: |
        =Set(varActiveScreen, "Profile"); Set(varUserProfile, {ID: varCurrentUser.ID, FullName: varCurrentUser.FullName, Email: varCurrentUser.Email, Phone: "0901234567", Department: varCurrentUser.Department, Position: "Nhân viên IT", Role: varCurrentUser.Role, Manager: "Trần Thị Bình", JoinDate: "01/01/2023", Status: "Active"}); Set(varLeaveBalance, {Year: 2024, TotalDays: 12, UsedDays: 3, RemainingDays: 9, UnpaidDays: 0, CarryOverDays: 2}); Set(varLeaveHistory, Table({ID: "LR001", Date: "15/01/2024", EndDate: "17/01/2024", Days: 3, Type: "Nghỉ phép năm", Status: "Approved", Reason: "Nghỉ lễ Tết"}, {ID: "LR003", Date: "10/03/2024", EndDate: "15/03/2024", Days: 6, Type: "Nghỉ phép năm", Status: "Rejected", Reason: "Du lịch gia đình"}, {ID: "LR004", Date: "05/04/2024", EndDate: "05/04/2024", Days: 1, Type: "Nghỉ ốm", Status: "Approved", Reason: "Khám bệnh"}, {ID: "LR005", Date: "25/04/2024", EndDate: "26/04/2024", Days: 2, Type: "Nghỉ phép năm", Status: "Pending", Reason: "Nghỉ cuối tuần dài"})); Set(varIsEditing, false)
    Children:
      - 'Profile.Header':
          Control: CanvasComponent
          ComponentName: HeaderComponent
          Properties:
            X: =Parent.X
            Y: =Parent.Y
            Width: =Parent.Width
            Height: =Parent.Height * 0.08
            Title: ="Thông tin cá nhân"
            UserName: =varCurrentUser.FullName
            UserRole: =varCurrentUser.Role
            ShowNotification: =true
            NotificationCount: =2
            
      - 'Profile.Navigation':
          Control: CanvasComponent
          ComponentName: NavigationComponent
          Properties:
            X: =Parent.X
            Y: ='Profile.Header'.Y + 'Profile.Header'.Height
            Width: =Parent.Width * 0.25
            Height: =Parent.Height - 'Profile.Header'.Height
            UserRole: =varCurrentUser.Role
            ActiveScreen: =varActiveScreen
            OnNavigate: =Set(varNavigateAction, true)
            
      - 'Profile.Content.Container':
          Control: GroupContainer
          Variant: ManualLayout
          Properties:
            X: ='Profile.Navigation'.X + 'Profile.Navigation'.Width
            Y: ='Profile.Header'.Y + 'Profile.Header'.Height
            Width: =Parent.Width - 'Profile.Navigation'.Width
            Height: =Parent.Height - 'Profile.Header'.Height
            Fill: =RGBA(248, 250, 252, 1)
          Children:
            - 'Profile.Content.Title':
                Control: Label
                Properties:
                  X: =Parent.X + Parent.Width * 0.03
                  Y: =Parent.Y + Parent.Height * 0.04
                  Width: =Parent.Width * 0.94
                  Height: =Parent.Height * 0.08
                  Text: ="Thông tin cá nhân"
                  Color: =RGBA(32, 54, 71, 1)
                  Font: =Font.'Open Sans'
                  FontWeight: =FontWeight.Bold
                  Size: =Parent.Height * 0.05
                  
            - 'Profile.Main.Container':
                Control: GroupContainer
                Variant: ManualLayout
                Properties:
                  X: =Parent.X + Parent.Width * 0.03
                  Y: ='Profile.Content.Title'.Y + 'Profile.Content.Title'.Height + Parent.Height * 0.03
                  Width: =Parent.Width * 0.94
                  Height: =Parent.Height * 0.8
                Children:
                  - 'Profile.Info.Container':
                      Control: GroupContainer
                      Variant: ManualLayout
                      Properties:
                        X: =Parent.X
                        Y: =Parent.Y
                        Width: =Parent.Width * 0.58
                        Height: =Parent.Height
                        Fill: =RGBA(255, 255, 255, 1)
                        BorderColor: =RGBA(230, 230, 230, 1)
                        BorderThickness: =Parent.Height * 0.001
                        DropShadow: =DropShadow.Light
                      Children:
                        - 'Profile.Info.Header':
                            Control: GroupContainer
                            Variant: ManualLayout
                            Properties:
                              X: =Parent.X + Parent.Width * 0.03
                              Y: =Parent.Y + Parent.Height * 0.03
                              Width: =Parent.Width * 0.94
                              Height: =Parent.Height * 0.1
                            Children:
                              - 'Profile.Info.Title':
                                  Control: Label
                                  Properties:
                                    X: =Parent.X
                                    Y: =Parent.Y
                                    Width: =Parent.Width * 0.7
                                    Height: =Parent.Height * 0.5
                                    Text: ="Thông tin nhân viên"
                                    Color: =RGBA(32, 54, 71, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Bold
                                    Size: =Parent.Height * 0.3
                                    
                              - 'Profile.Info.EditButton':
                                  Control: Classic/Button
                                  Properties:
                                    X: =Parent.Width * 0.75
                                    Y: =Parent.Y
                                    Width: =Parent.Width * 0.25
                                    Height: =Parent.Height * 0.6
                                    Text: =If(varIsEditing, "💾 Lưu", "✏️ Chỉnh sửa")
                                    Fill: =If(varIsEditing, RGBA(40, 167, 69, 1), RGBA(0, 120, 212, 1))
                                    Color: =RGBA(255, 255, 255, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Semibold
                                    Size: =Parent.Height * 0.2
                                    OnSelect: |
                                      =If(varIsEditing, Notify("Thông tin đã được cập nhật!", NotificationType.Success); Set(varIsEditing, false), Set(varIsEditing, true))
                                      
                        - 'Profile.Info.Avatar.Container':
                            Control: GroupContainer
                            Variant: ManualLayout
                            Properties:
                              X: =Parent.X + Parent.Width * 0.03
                              Y: ='Profile.Info.Header'.Y + 'Profile.Info.Header'.Height + Parent.Height * 0.03
                              Width: =Parent.Width * 0.94
                              Height: =Parent.Height * 0.15
                              Fill: =RGBA(240, 248, 255, 1)
                            Children:
                              - 'Profile.Info.Avatar':
                                  Control: Circle
                                  Properties:
                                    X: =Parent.X + Parent.Width * 0.03
                                    Y: =(Parent.Height - Self.Height) / 2
                                    Width: =Parent.Height * 0.8
                                    Height: =Parent.Height * 0.8
                                    Fill: =RGBA(0, 120, 212, 1)
                                    
                              - 'Profile.Info.Avatar.Icon':
                                  Control: Classic/Icon
                                  Properties:
                                    X: ='Profile.Info.Avatar'.X + ('Profile.Info.Avatar'.Width - Self.Width) / 2
                                    Y: ='Profile.Info.Avatar'.Y + ('Profile.Info.Avatar'.Height - Self.Height) / 2
                                    Width: =Parent.Height * 0.4
                                    Height: =Parent.Height * 0.4
                                    Icon: =Icon.Person
                                    Color: =RGBA(255, 255, 255, 1)
                                    
                              - 'Profile.Info.Avatar.Name':
                                  Control: Label
                                  Properties:
                                    X: ='Profile.Info.Avatar'.X + 'Profile.Info.Avatar'.Width + Parent.Width * 0.03
                                    Y: =Parent.Y + Parent.Height * 0.2
                                    Width: =Parent.Width * 0.7
                                    Height: =Parent.Height * 0.4
                                    Text: =varUserProfile.FullName
                                    Color: =RGBA(32, 54, 71, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Bold
                                    Size: =Parent.Height * 0.3
                                    
                              - 'Profile.Info.Avatar.Role':
                                  Control: Label
                                  Properties:
                                    X: ='Profile.Info.Avatar.Name'.X
                                    Y: ='Profile.Info.Avatar.Name'.Y + 'Profile.Info.Avatar.Name'.Height
                                    Width: =Parent.Width * 0.6
                                    Height: =Parent.Height * 0.3
                                    Text: =varUserProfile.Position & " - " & varUserProfile.Department
                                    Color: =RGBA(108, 117, 125, 1)
                                    Font: =Font.'Open Sans'
                                    Size: =Parent.Height * 0.2
                                    
                              - 'Profile.Info.Avatar.Status':
                                  Control: Label
                                  Properties:
                                    X: ='Profile.Info.Avatar.Name'.X
                                    Y: ='Profile.Info.Avatar.Role'.Y + 'Profile.Info.Avatar.Role'.Height + Parent.Height * 0.08
                                    Width: =Parent.Width * 0.2
                                    Height: =Parent.Height * 0.25
                                    Text: =varUserProfile.Status
                                    Color: =RGBA(255, 255, 255, 1)
                                    Fill: =RGBA(40, 167, 69, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Semibold
                                    Size: =Parent.Height * 0.15
                                    Align: =Align.Center
                                    
                        - 'Profile.Info.Details.Container':
                            Control: GroupContainer
                            Variant: ManualLayout
                            Properties:
                              X: =Parent.X + Parent.Width * 0.03
                              Y: ='Profile.Info.Avatar.Container'.Y + 'Profile.Info.Avatar.Container'.Height + Parent.Height * 0.03
                              Width: =Parent.Width * 0.94
                              Height: =Parent.Height * 0.65
                            Children:
                              - 'Profile.Info.Field.ID':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X
                                    Y: =Parent.Y
                                    Width: =Parent.Width
                                    Height: =Parent.Height * 0.15
                                  Children:
                                    - 'Profile.Info.Field.ID.Label':
                                        Control: Label
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.3
                                          Height: =Parent.Height * 0.6
                                          Text: ="Mã nhân viên:"
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          FontWeight: =FontWeight.Semibold
                                          Size: =Parent.Height * 0.25
                                          
                                    - 'Profile.Info.Field.ID.Value':
                                        Control: Label
                                        Properties:
                                          X: ='Profile.Info.Field.ID.Label'.X + 'Profile.Info.Field.ID.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Text: =varUserProfile.ID
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          
                              - 'Profile.Info.Field.Email':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X
                                    Y: ='Profile.Info.Field.ID'.Y + 'Profile.Info.Field.ID'.Height
                                    Width: =Parent.Width
                                    Height: =Parent.Height * 0.15
                                  Children:
                                    - 'Profile.Info.Field.Email.Label':
                                        Control: Label
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.3
                                          Height: =Parent.Height * 0.6
                                          Text: ="Email:"
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          FontWeight: =FontWeight.Semibold
                                          Size: =Parent.Height * 0.25
                                          
                                    - 'Profile.Info.Field.Email.Value':
                                        Control: Label
                                        Properties:
                                          X: ='Profile.Info.Field.Email.Label'.X + 'Profile.Info.Field.Email.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Text: =varUserProfile.Email
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          Visible: =Not(varIsEditing)
                                          
                                    - 'Profile.Info.Field.Email.Input':
                                        Control: Classic/TextInput
                                        Properties:
                                          X: ='Profile.Info.Field.Email.Label'.X + 'Profile.Info.Field.Email.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Default: =varUserProfile.Email
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          BorderColor: =RGBA(230, 230, 230, 1)
                                          Visible: =varIsEditing
                                          
                              - 'Profile.Info.Field.Phone':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X
                                    Y: ='Profile.Info.Field.Email'.Y + 'Profile.Info.Field.Email'.Height
                                    Width: =Parent.Width
                                    Height: =Parent.Height * 0.15
                                  Children:
                                    - 'Profile.Info.Field.Phone.Label':
                                        Control: Label
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.3
                                          Height: =Parent.Height * 0.6
                                          Text: ="Số điện thoại:"
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          FontWeight: =FontWeight.Semibold
                                          Size: =Parent.Height * 0.25
                                          
                                    - 'Profile.Info.Field.Phone.Value':
                                        Control: Label
                                        Properties:
                                          X: ='Profile.Info.Field.Phone.Label'.X + 'Profile.Info.Field.Phone.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Text: =varUserProfile.Phone
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          Visible: =Not(varIsEditing)
                                          
                                    - 'Profile.Info.Field.Phone.Input':
                                        Control: Classic/TextInput
                                        Properties:
                                          X: ='Profile.Info.Field.Phone.Label'.X + 'Profile.Info.Field.Phone.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Default: =varUserProfile.Phone
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          BorderColor: =RGBA(230, 230, 230, 1)
                                          Visible: =varIsEditing
                                          
                              - 'Profile.Info.Field.Manager':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X
                                    Y: ='Profile.Info.Field.Phone'.Y + 'Profile.Info.Field.Phone'.Height
                                    Width: =Parent.Width
                                    Height: =Parent.Height * 0.15
                                  Children:
                                    - 'Profile.Info.Field.Manager.Label':
                                        Control: Label
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.3
                                          Height: =Parent.Height * 0.6
                                          Text: ="Quản lý trực tiếp:"
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          FontWeight: =FontWeight.Semibold
                                          Size: =Parent.Height * 0.25
                                          
                                    - 'Profile.Info.Field.Manager.Value':
                                        Control: Label
                                        Properties:
                                          X: ='Profile.Info.Field.Manager.Label'.X + 'Profile.Info.Field.Manager.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Text: =varUserProfile.Manager
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          
                              - 'Profile.Info.Field.JoinDate':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X
                                    Y: ='Profile.Info.Field.Manager'.Y + 'Profile.Info.Field.Manager'.Height
                                    Width: =Parent.Width
                                    Height: =Parent.Height * 0.15
                                  Children:
                                    - 'Profile.Info.Field.JoinDate.Label':
                                        Control: Label
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.3
                                          Height: =Parent.Height * 0.6
                                          Text: ="Ngày vào làm:"
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          FontWeight: =FontWeight.Semibold
                                          Size: =Parent.Height * 0.25
                                          
                                    - 'Profile.Info.Field.JoinDate.Value':
                                        Control: Label
                                        Properties:
                                          X: ='Profile.Info.Field.JoinDate.Label'.X + 'Profile.Info.Field.JoinDate.Label'.Width
                                          Y: =Parent.Y
                                          Width: =Parent.Width * 0.7
                                          Height: =Parent.Height * 0.6
                                          Text: =varUserProfile.JoinDate
                                          Color: =RGBA(32, 54, 71, 1)
                                          Font: =Font.'Open Sans'
                                          Size: =Parent.Height * 0.25
                                          
                  - 'Profile.Leave.Container':
                      Control: GroupContainer
                      Variant: ManualLayout
                      Properties:
                        X: ='Profile.Info.Container'.X + 'Profile.Info.Container'.Width + Parent.Width * 0.02
                        Y: =Parent.Y
                        Width: =Parent.Width * 0.4
                        Height: =Parent.Height
                      Children:
                        - 'Profile.Leave.Balance.Container':
                            Control: GroupContainer
                            Variant: ManualLayout
                            Properties:
                              X: =Parent.X
                              Y: =Parent.Y
                              Width: =Parent.Width
                              Height: =Parent.Height * 0.5
                              Fill: =RGBA(255, 255, 255, 1)
                              BorderColor: =RGBA(230, 230, 230, 1)
                              BorderThickness: =Parent.Height * 0.001
                              DropShadow: =DropShadow.Light
                            Children:
                              - 'Profile.Leave.Balance.Title':
                                  Control: Label
                                  Properties:
                                    X: =Parent.X + Parent.Width * 0.05
                                    Y: =Parent.Y + Parent.Height * 0.05
                                    Width: =Parent.Width * 0.9
                                    Height: =Parent.Height * 0.1
                                    Text: ="Số ngày phép năm " & varLeaveBalance.Year
                                    Color: =RGBA(32, 54, 71, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Bold
                                    Size: =Parent.Height * 0.08
                                    
                              - 'Profile.Leave.Balance.Stats':
                                  Control: GroupContainer
                                  Variant: ManualLayout
                                  Properties:
                                    X: =Parent.X + Parent.Width * 0.05
                                    Y: ='Profile.Leave.Balance.Title'.Y + 'Profile.Leave.Balance.Title'.Height + Parent.Height * 0.05
                                    Width: =Parent.Width * 0.9
                                    Height: =Parent.Height * 0.75
                                  Children:
                                    - 'Profile.Leave.Balance.Total':
                                        Control: CanvasComponent
                                        ComponentName: StatsCardComponent
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.Width
                                          Height: =Parent.Height * 0.2
                                          Title: ="Tổng số ngày"
                                          Value: =varLeaveBalance.TotalDays
                                          Icon: ="Calendar"
                                          Color: =RGBA(0, 120, 212, 1)
                                          
                                    - 'Profile.Leave.Balance.Used':
                                        Control: CanvasComponent
                                        ComponentName: StatsCardComponent
                                        Properties:
                                          X: =Parent.X
                                          Y: ='Profile.Leave.Balance.Total'.Y + 'Profile.Leave.Balance.Total'.Height + Parent.Height * 0.05
                                          Width: =Parent.Width
                                          Height: =Parent.Height * 0.2
                                          Title: ="Số ngày đã sử dụng"
                                          Value: =varLeaveBalance.UsedDays
                                          Icon: ="CheckBadge"
                                          Color: =RGBA(40, 167, 69, 1)
                                          
                                    - 'Profile.Leave.Balance.Remaining':
                                        Control: CanvasComponent
                                        ComponentName: StatsCardComponent
                                        Properties:
                                          X: =Parent.X
                                          Y: ='Profile.Leave.Balance.Used'.Y + 'Profile.Leave.Balance.Used'.Height + Parent.Height * 0.05
                                          Width: =Parent.Width
                                          Height: =Parent.Height * 0.2
                                          Title: ="Số ngày còn lại"
                                          Value: =varLeaveBalance.RemainingDays
                                          Icon: ="Clock"
                                          Color: =RGBA(255, 193, 7, 1)
                                          
                                    - 'Profile.Leave.Balance.CarryOver':
                                        Control: CanvasComponent
                                        ComponentName: StatsCardComponent
                                        Properties:
                                          X: =Parent.X
                                          Y: ='Profile.Leave.Balance.Remaining'.Y + 'Profile.Leave.Balance.Remaining'.Height + Parent.Height * 0.05
                                          Width: =Parent.Width
                                          Height: =Parent.Height * 0.2
                                          Title: ="Chuyển từ năm trước"
                                          Value: =varLeaveBalance.CarryOverDays
                                          Icon: ="People"
                                          Color: =RGBA(108, 117, 125, 1)
                                          
                        - 'Profile.Leave.History.Container':
                            Control: GroupContainer
                            Variant: ManualLayout
                            Properties:
                              X: =Parent.X
                              Y: ='Profile.Leave.Balance.Container'.Y + 'Profile.Leave.Balance.Container'.Height + Parent.Height * 0.03
                              Width: =Parent.Width
                              Height: =Parent.Height * 0.45
                              Fill: =RGBA(255, 255, 255, 1)
                              BorderColor: =RGBA(230, 230, 230, 1)
                              BorderThickness: =Parent.Height * 0.001
                              DropShadow: =DropShadow.Light
                            Children:
                              - 'Profile.Leave.History.Title':
                                  Control: Label
                                  Properties:
                                    X: =Parent.X + Parent.Width * 0.05
                                    Y: =Parent.Y + Parent.Height * 0.05
                                    Width: =Parent.Width * 0.9
                                    Height: =Parent.Height * 0.1
                                    Text: ="Lịch sử nghỉ phép"
                                    Color: =RGBA(32, 54, 71, 1)
                                    Font: =Font.'Open Sans'
                                    FontWeight: =FontWeight.Bold
                                    Size: =Parent.Height * 0.08
                                    
                              - 'Profile.Leave.History.Gallery':
                                  Control: Gallery
                                  Variant: Vertical
                                  Properties:
                                    X: =Parent.X + Parent.Width * 0.05
                                    Y: ='Profile.Leave.History.Title'.Y + 'Profile.Leave.History.Title'.Height + Parent.Height * 0.03
                                    Width: =Parent.Width * 0.9
                                    Height: =Parent.Height * 0.8
                                    Items: =varLeaveHistory
                                    TemplateSize: =Parent.Height * 0.15
                                    TemplatePadding: =Parent.Height * 0.01
                                  Children:
                                    - 'Gallery.History.Container':
                                        Control: GroupContainer
                                        Variant: ManualLayout
                                        Properties:
                                          X: =Parent.X
                                          Y: =Parent.Y
                                          Width: =Parent.TemplateWidth
                                          Height: =Parent.TemplateHeight
                                          Fill: =RGBA(248, 249, 250, 1)
                                          BorderColor: =RGBA(230, 230, 230, 1)
                                          BorderThickness: =Parent.Height * 0.01
                                        Children:
                                          - 'Gallery.History.ID':
                                              Control: Label
                                              Properties:
                                                X: =Parent.X + Parent.Width * 0.03
                                                Y: =Parent.Y + Parent.Height * 0.08
                                                Width: =Parent.Width * 0.3
                                                Height: =Parent.Height * 0.3
                                                Text: =ThisItem.ID
                                                Color: =RGBA(0, 120, 212, 1)
                                                Font: =Font.'Open Sans'
                                                FontWeight: =FontWeight.Bold
                                                Size: =Parent.Height * 0.15
                                                
                                          - 'Gallery.History.Status':
                                              Control: Label
                                              Properties:
                                                X: =Parent.Width * 0.75
                                                Y: =Parent.Y + Parent.Height * 0.08
                                                Width: =Parent.Width * 0.22
                                                Height: =Parent.Height * 0.3
                                                Text: =Switch(ThisItem.Status,
                                                  "Approved", "Đã sử dụng",
                                                  "Pending", "Chưa sử dụng",
                                                  "Rejected", "Từ chối",
                                                  "Không xác định", "Không xác định")
                                                Color: =RGBA(255, 255, 255, 1)
                                                Fill: =Switch(ThisItem.Status,
                                                  "Approved", RGBA(40, 167, 69, 1),
                                                  "Pending", RGBA(255, 193, 7, 1),
                                                  "Rejected", RGBA(220, 53, 69, 1),
                                                  RGBA(108, 117, 125, 1))
                                                Font: =Font.'Open Sans'
                                                FontWeight: =FontWeight.Semibold
                                                Size: =Parent.Height * 0.12
                                                Align: =Align.Center
                                                
                                          - 'Gallery.History.Period':
                                              Control: Label
                                              Properties:
                                                X: =Parent.X + Parent.Width * 0.03
                                                Y: ='Gallery.History.ID'.Y + 'Gallery.History.ID'.Height + Parent.Height * 0.03
                                                Width: =Parent.Width * 0.94
                                                Height: =Parent.Height * 0.25
                                                Text: =ThisItem.Date & " - " & ThisItem.EndDate & " (" & ThisItem.Days & " ngày)"
                                                Color: =RGBA(32, 54, 71, 1)
                                                Font: =Font.'Open Sans'
                                                FontWeight: =FontWeight.Semibold
                                                Size: =Parent.Height * 0.15
                                                
                                          - 'Gallery.History.Type':
                                              Control: Label
                                              Properties:
                                                X: =Parent.X + Parent.Width * 0.03
                                                Y: ='Gallery.History.Period'.Y + 'Gallery.History.Period'.Height + Parent.Height * 0.03
                                                Width: =Parent.Width * 0.94
                                                Height: =Parent.Height * 0.2
                                                Text: =ThisItem.Type
                                                Color: =RGBA(108, 117, 125, 1)
                                                Font: =Font.'Open Sans'
                                                Size: =Parent.Height * 0.12
                                                
                                          - 'Gallery.History.Reason':
                                              Control: Label
